---
layout: default
title: Contato
javascript: $(document).ready(function(){ $('#contato').validate() })
---
%form#contato{ :action => 'http://participe.de/events/ii-conag-congresso-nacional-de-gastronomia/contacts', :method => 'post' }
  %fieldset
    %label Nome:
    %input.text.required{ :name => 'nome' }
    %label E-mail:
    %input.text.email.required{ :name => 'email' }
    %label Telefone:
    %input.text.required{ :name => 'telefone' }
    %label Mensagem:
    %textarea.text.required{ :name => 'mensagem' }
    %br
    %br
    %input#enviar{ :name => 'submit', :value => 'enviar', :type => 'submit' }


